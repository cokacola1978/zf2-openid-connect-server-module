{
    "name" : "ivan-novakov/zf2-openid-connect-server-module",
    "description" : "OpenID Connect server implementation in PHP as a Zend Framework 2 module",
    "homepage" : "https://github.com/ivan-novakov/zf2-openid-connect-server-module",
    "license" : "BSD-3-Clause",
    "keywords" : [
        "openid connect", "authentication", "oauth2", "zf2"
    ],
    "authors" : [
        {
            "name" : "Ivan Novakov",
            "homepage" : "http://novakov.cz/"
        }
    ],

    "require" : {
        "php" : ">=5.3.3",
        "zendframework/zend-servicemanager" : "~2.3",
        "zendframework/zend-serializer" : "~2.3",
        "zendframework/zend-mvc" : "~2.3",
        "zendframework/zend-filter" : "~2.3",
        "zendframework/zend-inputfilter" : "~2.3",
        "zendframework/zend-log" : "~2.3",
        "zendframework/zend-db" : "~2.3",
        "zendframework/zend-json" : "~2.3",
        "zendframework/zend-http" : "~2.3",
        "zendframework/zend-stdlib" : "~2.3",
        "zendframework/zend-session" : "~2.3",
        "zendframework/zend-crypt" : "~2.3"
    },

    "require-dev" : {
        "phpunit/phpunit" : "~4.0"
    },

    "autoload" : {
        "psr-0" : {
            "InoOicServer\\" : "src/"
        },
        "classmap" : [
            "Module.php"
        ]
    }

}
